{
	"verbose": true,
	"collectCoverage": true,
	"coverageReporters": ["lcov", "json"],
	"collectCoverageFrom": [
		"**/src/**",
		"!**/src/**/*.d.ts",
		"!**/node_modules/**/*.d.ts"
	],
	"testRegex": "(/tests?/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
	"moduleFileExtensions": [
		"ts",
		"tsx",
		"js",
		"jsx",
		"json",
		"node"
  ],
  "moduleNameMapper": {
  	"@harpsealjs/(.*)": "<rootDir>/../$1"
  }
}